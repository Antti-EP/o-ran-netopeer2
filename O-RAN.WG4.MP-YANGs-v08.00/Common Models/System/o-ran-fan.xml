<nacm xmlns="urn:ietf:params:xml:ns:yang:ietf-netconf-acm">
	<rule-list>
		<name>fan-acl</name>
		<group>sudo</group>
		<group>nms</group>
		<group>fm-pm</group>
		<group>smo</group>
		<group>hybrid-odu</group>
		<rule>
			<name>allow-fan-read</name>
			<module-name>o-ran-fan</module-name>
			<access-operations>
				read
			</access-operations>
			<action>permit</action>
		</rule>
	</rule-list>
</nacm>
